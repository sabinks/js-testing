{
  "rules": {
    "space-before-function-paren": "off",
    "semi": ["error", "always"],
    "no-console": "off"
  },
  "extends":[
   "eslint:recommended",
   "plugin:@typescript-eslint/recommended"
  ],
  "plugins": ["@typescript-eslint"],
  "root": true,
  "ignorePatterns": ["dist/"],
  "parser": "@typescript-eslint",
  "parserOptions": {},
  "env": {
    "node": true,
    "browser": true
  }
}